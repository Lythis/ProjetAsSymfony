{% extends "base.html.twig" %}
{% block body %}
    {{ form_start(categoryForm) }}
    <div class="container">
        <div class="row">
            <div class="col-md-6 col-12">
                <div class="mb-2">
                    {{ form_row(categoryForm.label) }}
                </div>
                <div class="mb-2 mt-2">
                    {{ form_row(categoryForm.submit) }}
                </div>
                <div class="mb-2 mt-1">
                    <span class="text-danger">* Champs obligatoires</span>
                </div>
            </div>
        </div>
    </div>
    {{ form_end(categoryForm) }}
{% endblock %}